{
	"$schema": "https://json.schemastore.org/web-manifest-combined.json",
	"id": "https://brand.torontojs.com/?id=aa87dd33-35a8-4710-a835-88e98078e582",
	"scope": "https://brand.torontojs.com/?utm_source=pwa&utm_medium=install&utm_campaign=web_app_manifest",
	"name": "Toronto JS - Brand Manual & Style Guide",
	"short_name": "TJS Manual",
	"lang": "en-US",
	"dir": "ltr",
	"description": "The Toronto JS Brand Manual & Style Guide is a living document that describes the style we use at Toronto JS. It is intended to be a reference for current and future contributors to the Toronto JS community who wish to develop or create anything using Toronto JS branding.",
	"categories": ["development", "utilities", "reference"],
	"display": "standalone",
	"orientation": "portrait",
	"display_override": ["window-controls-overlay"],
	"background_color": "#252525",
	"theme_color": "#ee2e24",
	"prefer_related_applications": false,
	"icons": [
		{
			"src": "icons/transparent/manifest-icon-192.png",
			"sizes": "192x192",
			"type": "image/png",
			"purpose": "any"
		},
		{
			"src": "icons/maskable/manifest-icon-192.png",
			"sizes": "192x192",
			"type": "image/png",
			"purpose": "maskable"
		},
